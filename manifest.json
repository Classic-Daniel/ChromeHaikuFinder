{
    "name": "Haiku Finder",
    "description": "Finds haikus in the current page",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
      "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/haiku16.png",
            "32": "/images/haiku32.png",
            "48": "/images/haiku48.png",
            "128": "/images/haiku128.png"
          }
    },  
    "icons": {
        "16": "/images/haiku16.png",
        "32": "/images/haiku32.png",
        "48": "/images/haiku48.png",
        "128": "/images/haiku128.png"
      }
  }